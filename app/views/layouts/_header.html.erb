<header>
<nav>

  <div class="nav nav-links">
    <a href="" class="logo-click">Bokeh</a>
    <a href="">Explore</a>
  </div>

  <div class="nav nav-tools">
    <% if signed_in? %>
      <a href="<%= user_url(current_user) %>"><%= current_user.name %></a>
      <%= button_to "Sign Out", session_url, :method => :delete %>
    <% else %>
      <%= link_to "Sign Up", new_user_url %>
    <% end %>
  </div>

</nav>
</header>
